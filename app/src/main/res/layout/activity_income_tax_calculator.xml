<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:background="@drawable/background"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/main"
            android:layout_width="match_parent"
            android:layout_height="1272dp"
            tools:context=".IncomeTaxCalculatorActivity">

            <ImageView
                android:id="@+id/imageView"
                android:layout_width="113dp"
                android:layout_height="119dp"
                android:contentDescription="@string/logo2"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.053"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@drawable/logo" />

            <TextView
                android:id="@+id/textView7"
                android:layout_width="186dp"
                android:layout_height="28dp"
                android:layout_marginTop="8dp"
                android:text="Payable Tax"
                android:textAlignment="center"
                android:textColor="#781D00"
                android:textSize="20sp"
                android:textStyle="bold"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.497"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/imageView"
                tools:ignore="HardcodedText,TextSizeCheck" />

            <TextView
                android:id="@+id/textView9"
                android:layout_width="175dp"
                android:layout_height="28dp"
                android:layout_marginTop="16dp"
                android:text="DATE"
                android:textAlignment="center"
                android:textColor="#000000"
                android:textSize="16sp"
                android:textStyle="bold"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView7"
                tools:ignore="HardcodedText,TextSizeCheck" />

            <EditText
                android:id="@+id/editTextNumber3"
                android:layout_width="174dp"
                android:layout_height="48dp"
                android:layout_marginTop="8dp"
                android:autofillHints="0"
                android:ems="10"
                android:hint="@string/nun"
                android:inputType="date"
                android:textAlignment="center"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.497"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView9"
                tools:ignore="LabelFor,DuplicateSpeakableTextCheck" />

            <TextView
                android:id="@+id/textView10"
                android:layout_width="172dp"
                android:layout_height="27dp"
                android:layout_marginTop="20dp"
                android:text="Gross Income"
                android:textColor="#070707"
                android:textSize="16sp"
                android:textStyle="bold"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.138"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/editTextNumber3"
                tools:ignore="HardcodedText,TextSizeCheck" />

            <EditText
                android:id="@+id/editTextNumberDecimal2"
                android:layout_width="341dp"
                android:layout_height="48dp"
                android:layout_marginTop="12dp"
                android:autofillHints="0"
                android:ems="10"
                android:hint="@string/nun"
                android:inputType="numberDecimal"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.355"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView10"
                tools:ignore="LabelFor" />

            <TextView
                android:id="@+id/textView11"
                android:layout_width="138dp"
                android:layout_height="32dp"
                android:layout_marginTop="8dp"
                android:text="DEDUCTIONS"
                android:textAlignment="center"
                android:textColor="#8E2301"
                android:textSize="16sp"
                android:textStyle="bold"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.472"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/editTextNumberDecimal2"
                tools:ignore="HardcodedText,TextSizeCheck" />

            <TextView
                android:id="@+id/textView12"
                android:layout_width="183dp"
                android:layout_height="27dp"
                android:layout_marginTop="20dp"
                android:text="Personal Relief"
                android:textColor="#000000"
                android:textSize="16sp"
                android:textStyle="bold"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.144"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView11"
                tools:ignore="HardcodedText,TextSizeCheck" />

            <EditText
                android:id="@+id/editTextNumber4"
                android:layout_width="348dp"
                android:layout_height="48dp"
                android:layout_marginTop="4dp"
                android:autofillHints="0"
                android:ems="10"
                android:hint="@string/nun"
                android:inputType="number"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.38"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView12" />

            <TextView
                android:id="@+id/textView13"
                android:layout_width="177dp"
                android:layout_height="29dp"
                android:layout_marginTop="20dp"
                android:text="Insurance Relief"
                android:textColor="#000000"
                android:textSize="16sp"
                android:textStyle="bold"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.141"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/editTextNumber4"
                tools:ignore="HardcodedText,TextSizeCheck" />

            <EditText
                android:id="@+id/editTextNumber5"
                android:layout_width="345dp"
                android:layout_height="48dp"
                android:layout_marginTop="4dp"
                android:autofillHints=""
                android:ems="10"
                android:hint="@string/nun"
                android:inputType="number"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.37"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView13" />

            <TextView
                android:id="@+id/textView14"
                android:layout_width="178dp"
                android:layout_height="26dp"
                android:layout_marginTop="16dp"
                android:text="Housing Relief"
                android:textColor="#000000"
                android:textSize="16sp"
                android:textStyle="bold"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.142"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/editTextNumber5"
                tools:ignore="HardcodedText,TextSizeCheck" />

            <EditText
                android:id="@+id/editTextNumber6"
                android:layout_width="344dp"
                android:layout_height="48dp"
                android:layout_marginStart="36dp"
                android:autofillHints=""
                android:ems="10"
                android:hint="@string/nun"
                android:inputType="number"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView14"
                tools:ignore="MissingConstraints" />

            <TextView
                android:id="@+id/textView15"
                android:layout_width="182dp"
                android:layout_height="23dp"
                android:layout_marginTop="16dp"
                android:text="Pension Contribution"
                android:textColor="#000000"
                android:textSize="16sp"
                android:textStyle="bold"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.14"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/editTextNumber6"
                tools:ignore="HardcodedText,TextSizeCheck" />

            <EditText
                android:id="@+id/editTextNumber7"
                android:layout_width="342dp"
                android:layout_height="48dp"
                android:layout_marginTop="4dp"
                android:autofillHints="0"
                android:ems="10"
                android:hint="@string/nun"
                android:inputType="number"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.349"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView15"
                tools:ignore="VisualLintBounds" />

            <Button
                android:id="@+id/button5"
                android:layout_width="226dp"
                android:layout_height="62dp"
                android:layout_marginTop="12dp"
                android:backgroundTint="#009688"
                android:text="CALCULATE"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/editTextNumber7"
                tools:ignore="HardcodedText,VisualLintBounds" />

            <TextView
                android:id="@+id/textView16"
                android:layout_width="145dp"
                android:layout_height="41dp"
                android:layout_marginStart="20dp"
                android:layout_marginTop="76dp"
                android:lineSpacingExtra="16sp"
                android:text="Taxable Income:"
                android:textAlignment="center"
                android:textColor="#F44336"
                android:textSize="16sp"
                android:textStyle="bold"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/button5"
                tools:ignore="HardcodedText,TextSizeCheck,VisualLintBounds" />

            <EditText
                android:id="@+id/editTextNumber8"
                android:layout_width="249dp"
                android:layout_height="50dp"
                android:layout_marginStart="4dp"
                android:layout_marginTop="64dp"
                android:autofillHints=""
                android:background="@android:color/transparent"
                android:ems="10"
                android:fontFamily="serif"
                android:hint="@string/nun"
                android:inputType="number"
                android:minHeight="48dp"
                android:textAlignment="center"
                android:textColor="#014704"
                android:textSize="34sp"
                android:textStyle="bold"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toEndOf="@+id/textView16"
                app:layout_constraintTop_toBottomOf="@+id/button5"
                tools:ignore="MissingConstraints" />

            <TextView
                android:id="@+id/textView17"
                android:layout_width="128dp"
                android:layout_height="37dp"
                android:layout_marginStart="12dp"
                android:layout_marginTop="40dp"
                android:text="Payable Tax"
                android:textAlignment="center"
                android:textColor="#FF5722"
                android:textSize="16sp"
                android:textStyle="bold"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView16"
                tools:ignore="HardcodedText" />

            <EditText
                android:id="@+id/editTextNumber9"
                android:layout_width="230dp"
                android:layout_height="58dp"
                android:layout_marginStart="9dp"
                android:layout_marginTop="20dp"
                android:autofillHints="0"
                android:background="@android:color/transparent"
                android:ems="10"
                android:fontFamily="serif"
                android:hint="@string/nun"
                android:inputType="number"
                android:textAlignment="center"
                android:textColor="#004300"
                android:textSize="34sp"
                android:textStyle="bold"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="1.0"
                app:layout_constraintStart_toEndOf="@+id/textView17"
                app:layout_constraintTop_toBottomOf="@+id/editTextNumber8" />

            <TextView
                android:id="@+id/textView18"
                android:layout_width="150dp"
                android:layout_height="30dp"
                android:text="Basic Salary"
                android:textAlignment="center"
                android:textColor="#F44336"
                android:textSize="16sp"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/editTextNumber10"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView17"
                app:layout_constraintVertical_bias="0.164"
                tools:ignore="HardcodedText" />

            <EditText
                android:id="@+id/editTextNumber10"
                android:layout_width="231dp"
                android:layout_height="57dp"
                android:autofillHints="0"
                android:background="@android:color/transparent"
                android:ems="10"
                android:fontFamily="serif"
                android:hint="@string/nun"
                android:inputType="number"
                android:textAlignment="center"
                android:textColor="#002E03"
                android:textSize="34sp"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/editTextNumber9"
                app:layout_constraintVertical_bias="0.119" />

        </androidx.constraintlayout.widget.ConstraintLayout>

    </LinearLayout>
</ScrollView>
